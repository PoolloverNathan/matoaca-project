;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Gem",
"language": "GDScript",
"path": "res://scripts/gem.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://player/player_movement.gd"
}, {
"base": "Node",
"class": "Preferences",
"language": "GDScript",
"path": "res://scripts/Preferences.gd"
}, {
"base": "Node",
"class": "Savegame",
