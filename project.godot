}

[application]

config/name="Matoaca Project - Nathan"
run/main_scene="res://start.tscn"
config/icon="res://icon.png"
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )

[autoload]

BlackFade="*res://BlackFade.tscn"
PlayerService="*res://scripts/PlayerService.gd"
PauseMenu="*res://pause.tscn"

[debug]

settings/stdout/verbose_stdout.editor=true

[display]

window/energy_saving/keep_screen_on=false
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor]

scene_naming=2
convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "res://addons/tile_spawner/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://theme.tres"

